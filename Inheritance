class Vehicle {
    int speed = 60;

    void display() {
        System.out.println("This is a vehicle.");
    }
}

class Car extends Vehicle {
    String brand = "Toyota";

    void showDetails() {
        System.out.println("Brand: " + brand);
        System.out.println("Speed: " + speed);
    }
}

public class Main {
    public static void main(String[] args) {
        Car myCar = new Car();
        myCar.display();      // Inherited method
        myCar.showDetails();  // Subclass method
    }
}
