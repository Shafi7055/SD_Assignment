class BankAccount {
    private double balance;

    public void setBalance(double b) { balance = b; }
    public double getBalance() { return balance; }
}

public class Main {
    public static void main(String[] args) {
        BankAccount b = new BankAccount();
        b.setBalance(1000);
        System.out.println(b.getBalance());
    }
}
